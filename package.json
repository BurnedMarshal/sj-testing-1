{
  "name": "sj-testing-1",
  "version": "1.0.1",
  "description": "Testing and Versioning Lesson 1",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "prelint": "rimraf coverage",
    "lint": "node ./node_modules/.bin/eslint .",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcovonly mocha -- --timeout 5000 --report lcovonly --exit -R spec $(find ./tests -name \"*.spec.js\" -not -path \"./node_modules/*\")"
  },
  "keywords": [
    "express",
    "api"
  ],
  "author": "Daniele Manni",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-validator": "^5.3.1",
    "mongoose": "^5.4.20",
    "morgan": "^1.7.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "mocha": "^6.0.2",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2"
  }
}
